{% extends "layouts/default.twig" %}

{% block content %}
    
            <!--Page Title-->
            <section class="page-title" style="background-image:url(./assets/images/background/bg-page-title-sugar-cane2.jpg);">
              <div class="auto-container">
              <h1>About</h1>
              </div>
            </section>

            <!--About Section-->
            <section class="about-farm">
              <div class="auto-container">          
                <div class="row clearfix">
